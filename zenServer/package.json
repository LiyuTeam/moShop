{
  "name": "zenServer",
  "version": "1.0.0",
  "description": "a ts micro service by egg",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-zenServer",
    "stop": "egg-scripts stop --title=egg-server-zenServer",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "apollo-server-koa": "^2.14.3",
    "class-validator": "^0.14.0",
    "data-forge": "^1.8.9",
    "dataloader": "^2.0.0",
    "egg": "^2.6.1",
    "egg-graphql": "^2.8.0",
    "egg-scripts": "^2.6.0",
    "egg-ts-typeorm": "^1.1.12",
    "graphql": "^15.1.0",
    "lodash": "^4.17.15",
    "reflect-metadata": "^0.1.13",
    "seneca": "^3.21.2",
    "sqlite3": "^4.2.0",
    "tcb-admin-node": "^1.23.0",
    "type-graphql": "^1.0.0-rc.2",
    "typeorm": "^0.2.25",
    "vue-property-decorator": "^8.4.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.15.3",
    "@graphql-codegen/introspection": "1.15.3",
    "@graphql-codegen/typescript": "1.15.3",
    "@graphql-codegen/typescript-document-nodes": "1.15.3",
    "@graphql-codegen/typescript-mongodb": "1.15.3",
    "@graphql-codegen/typescript-resolvers": "1.15.3",
    "@graphql-codegen/typescript-type-graphql": "^1.15.3",
    "@types/mocha": "^2.2.40",
    "@types/node": "^14.0.6",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "eslint": "^6.7.2",
    "eslint-config-egg": "^8.0.0",
    "ts-graphql-plugin": "^1.12.0",
    "tslib": "^1.9.0",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "zico",
  "license": "MIT"
}
